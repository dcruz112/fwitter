<%= simple_form_for @user do |f| %>
  
  <br>

    <%= f.input :first_name, input_html: { maxlength: 20 }, label: 'First Name'%>
    <%= f.input :last_name, input_html: { maxlength: 20 } %>
    <%= f.input :netid, input_html: { maxlength: 7 }, disabled: true %>
    <%= f.input :handle, input_html: { maxlength: 15 } %>
    <%= f.input :biography, input_html: { maxlength: 160 } %>
    <%= f.input :current_location, input_html:{ maxlength: 20 } %>
    <%= f.submit 'Update Profile' %>

   
  
<% end %>

<br>
